{
  "main": "app/index.html",
  "name": "pxpsprintcli",
  "description": "Paxapos Print Client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "0.11.x"
  },
  "webkit": {
        "plugin": true
  },
    "scripts": {         
         "start": "nodewebkit",
         "spooler": "spooler"
  },
  "dependencies": {	  
    "bootstrap": "latest",
    "tmp": "latest",	  
    "jquery": "latest"
  },
  "bin": {
    "spooler.exe": "node_modules/.bin/spooler.exe",
    "spooler": "node_modules/.bin/spooler"
    },
  "os": [
    "win32",
    "linux"
  ],
  "window": {
    "show": true,
    "toolbar": true,
    "always-on-top": true,
    "width": 230,
    "height": 380,
    "position": "mouse",
    "frame": true,
    "resizable": false
  }
}
